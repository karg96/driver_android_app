<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fragment.Account">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/toolbar"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/vehilce"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="60dp"
                android:clickable="true"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/local_taxi_24_px" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="0dp"
                    android:fontFamily="@font/nunitosans_regular"
                    android:gravity="center_vertical"
                    android:paddingLeft="21dp"
                    android:text="@string/vehicle"
                    android:textColor="@color/colorBlack"
                    android:textSize="20sp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:clickable="true"
                android:orientation="horizontal"
                android:visibility="visible">

                <ImageView
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/file_document" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="0dp"
                    android:fontFamily="@font/nunitosans_regular"
                    android:gravity="center_vertical"
                    android:paddingLeft="21dp"
                    android:text="@string/document"
                    android:textColor="@color/colorBlack"
                    android:textSize="20sp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:clickable="true"
                android:orientation="horizontal"
                android:visibility="visible">

                <ImageView
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/payment_24_px" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="0dp"
                    android:fontFamily="@font/nunitosans_regular"
                    android:gravity="center_vertical"
                    android:paddingLeft="21dp"
                    android:text="@string/payment"
                    android:textColor="@color/colorBlack"
                    android:textSize="20sp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:clickable="true"
                android:orientation="horizontal"
                android:visibility="visible">

                <ImageView
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/info_24_px" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="0dp"
                    android:fontFamily="@font/nunitosans_regular"
                    android:gravity="center_vertical"
                    android:paddingLeft="21dp"
                    android:text="@string/editIno"
                    android:textColor="@color/colorBlack"
                    android:textSize="20sp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:clickable="true"
                android:orientation="horizontal"
                android:visibility="visible">

                <ImageView
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/bug_report_24_px" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="0dp"
                    android:fontFamily="@font/nunitosans_regular"
                    android:gravity="center_vertical"
                    android:paddingLeft="21dp"
                    android:text="@string/reportIssue"
                    android:textColor="@color/colorBlack"
                    android:textSize="20sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:clickable="true"
                android:orientation="horizontal"
                android:visibility="visible">


                <ImageView
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/info_24_px" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="0dp"
                    android:fontFamily="@font/nunitosans_regular"
                    android:gravity="center_vertical"
                    android:paddingLeft="21dp"
                    android:text="@string/about"
                    android:textColor="@color/colorBlack"
                    android:textSize="20sp" />


            </LinearLayout>


            <LinearLayout
                android:id="@+id/securityPrivacy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:clickable="true"
                android:orientation="horizontal"
                android:visibility="visible">


                <ImageView
                    android:layout_width="26dp"
                    android:layout_height="26dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="18dp"
                    android:src="@drawable/lock_24_px" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="0dp"
                    android:fontFamily="@font/nunitosans_regular"
                    android:gravity="center_vertical"
                    android:paddingLeft="21dp"
                    android:text="@string/security"
                    android:textColor="@color/colorBlack"
                    android:textSize="20sp" />


            </LinearLayout>


        </LinearLayout>

    </ScrollView>

</FrameLayout>
